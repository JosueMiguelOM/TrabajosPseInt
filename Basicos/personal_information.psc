Algoritmo personal_information
	Definir edad,telefono como entero;
	Definir nombre,email como texto;
	Escribir "Ingresa nombre";
	Leer nombre;
	Escribir "Ingresa email";
	Leer email;
	Escribir "Ingresa edad";
	Leer edad;
	Escribir "Ingresa telefono";
	Leer telefono;
	Escribir "Hola ", nombre " ,tu correo es: ", email " ,tu telefono es: ", telefono " y tienes :", edad " años"; 
FinAlgoritmo
