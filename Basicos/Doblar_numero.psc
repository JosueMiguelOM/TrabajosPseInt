Algoritmo Doblar_numero
	Escribir "Algoritmo Doblar Número";
	Escribir "===========================";
	Escribir "";
	
	Definir data Como Entero;
	Escribir "Ingresa el numero: ";
	Leer data;
	//Paso 2
	Definir res Como Entero;
	res <- data * 2 * 25;
	//Paso 3
	Escribir "El resultado es: ", res;
FinAlgoritmo
