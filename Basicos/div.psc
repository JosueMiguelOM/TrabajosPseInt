Algoritmo div
	Definir numero_1 Como entero;
	Definir numero_2 Como entero;
	Definir division Como entero;
	Definir residuo Como Entero;
	Escribir "numero 1";
	Leer numero_1;
	Escribir "numero 2";
	Leer numero_2;
	Escribir "primer numero: ", numero_1 " entre " numero_2;
	division = numero_1/numero_2;
	Escribir "El resultado es: ", division;
	residuo = numero_1 MOD numero_2;
	Escribir "El residuo es: " residuo;
FinAlgoritmo
